<!DOCTYPE html>
<html>
    <!--establishing the tile of the page-->
    <head>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
         <title> CV - Homepage </title>
         <!--loading the icons-->
         <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
         <!--link the Materialize framework--> <!--link the css file-->
         <link type="text/css" rel="stylesheet" href="css/materialize.min.css"  media="screen,projection"/> <!--Materialize style-->
       	 <link rel="stylesheet" href="mycss/style.css"> <!-- Resource style -->
         <link rel="stylesheet" type="text/css" href="mycss/cv2.css">
         
    </head>

    <!--the body of the page-->
    <body>
       <!--start of the navigation bar-->
       <% include ./partials/navigation %>
       <!--end of navigation-->

       <!-- page landing -->
       <% include ./partials/landing %>

        <!--parallax effect-->
        <div class="parallax-container" id="parallax1">
            <div class="parallax"><img src="images/background-paralax.jpg"></div>
        </div>
        <!--end of parallax effect-->

        <!--writing the intro-->
        <% include ./partials/intro %>
        <!--end of writing the intro-->

        <% include ./partials/timeline %>

        <!--projects-->
        <% include ./partials/projects %>
        <!--end of projects-->

        <!--parallax effect-->
        <% include ./partials/parallax %>
        <!--end of parallax effect-->

        <!--contact info-->
        <div id="contact" role="contentinfo" class="card-panel">
            <% include ./partials/contactInfo %>
        <!--end of personal information-->

        <!--contact form-->
            <% include ./partials/form %>   
        <!--end of form-->

        </div>

        <!--signature footer-->
        <% include ./partials/footer %>

        <!-- the modals for projects -->
       <% include ./partials/modalStructure %>

        <!--link the jQuery file-->
         <script src="js/jquery-3.1.1.js" type="text/javascript"></script>
         <script src="js/materialize.js" type="text/javascript"></script> <!--Materialize.js-->
         <script src="js/typed.js"></script> <!-- Typed.js -->
         <script src="js/modernizr.js"></script> <!-- Modernizr -->
         <script src="js/myjquery.js" type="text/javascript"></script>
         <script src="js/theModals.js" type="text/javascript"></script>
    </body>
</html>